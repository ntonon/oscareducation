    Text

  %button.btn.btn-success.add-question{ng-click: "createInput(1, topIndex, question)", type: "button", id:"addBox"}
    %span.glyphicon.glyphicon-plus
    Math

  %button.btn.btn-success.add-question{ng-click: "createInput(2, topIndex, question)", type: "button", id:"addBox"}
